{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "cd android && ./gradlew clean && cd .. && yarn start --reset-cache",
    "release-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "release-abb": "cd android && ./gradlew bundleRelease && cd ..",
    "release-ipa": "cd ios && fastlane release",
    "upgrade": "npm-run-all upgrade:*",
    "upgrade:minor": "npm-run-all upgrade:*@minor",
    "upgrade:patch": "npm-run-all upgrade:*@patch",
    "lib-v:check": "ncu -u",
    "lib-v:upgrade": "ncu -u --upgrade && yarn install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/lodash": "^4.14.202",
    "lodash": "^4.17.21",
    "moti": "^0.27.2",
    "react": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-native": "0.73.1",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-paper": "^5.11.6",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "i18next": "^23.7.12",
    "react-native-localize": "^3.0.4",
    "react-i18next": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  }
}